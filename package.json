    "scripts": {
        "start": "npm run  start:test",
        "start:test": "cat .env.develop > .env && cat src/config/testnet.json > src/config/config.json && react-scripts start",
        "start:main": "cat .env.production > .env && cat src/config/mainnet.json > src/config/config.json && react-scripts start",
        "build": "npm run build:test",
        "build:test": "cat .env.develop > .env && cat src/config/testnet.json > src/config/config.json && react-scripts build",
        "build:main": "cat .env.production > .env && cat src/config/mainnet.json > src/config/config.json && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "build-storybook-docs": "build-storybook -s public --docs",
        "lint": "eslint --ext .ts,.tsx  --ignore-path ./.eslintignore",
        "lint:fix": "eslint --fix --ext .ts,.tsx  --ignore-path ./.eslintignore",
        "prettier": "prettier .",
        "format": "npx prettier --write .",
        "format:check": "npm run prettier --check",
        "validate": "npm run format:check && npm run lint",
        "prepare": "husky install",
        "cypress": "cypress open",
        "cypress:run": "cypress run",
        "cypress:qa": "cypress open --config-file ./cypress.staging.json"
    },
    "resolutions": {
        "babel-loader": "8.1.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v && npm run validate && npm run format"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "yarn run lint:fix"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@storybook/addon-actions": "^6.3.2",
